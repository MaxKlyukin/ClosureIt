{
  "name": "ClosureIt",
  "description": "ClosureIt is a PHP library that can generate SQL statement from PHP closures. Not for production!",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Max K"
    }
  ],
  "require": {
    "php": ">=5.5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
    "psr-4": {
      "ClosureIt\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ClosureIt\\Test\\" : "test/"
    }
  }
}